{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "./pages/cart.tsx",
    "./pages/testimonial.tsx",
    "pages/404.tsx",
    "pages/_app.tsx",
    "app/store.tsx",
    "app/slices.tsx",
    "app/orderSlice.tsx",
    "utils/ErrorBoundry.js",
    "hooks/useAuth.js",
    "components/Header.js"
  ],
  "exclude": ["node_modules"]
}
